.itembox{
  height: 300rpx;
  width: 680rpx;
  border-radius: 20rpx;
  /* border: 1px solid skyblue; */
  background: #ffffff;
  margin-left: auto;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 40rpx;

}
.itembox>image{
  height: 230rpx;
  width: 230rpx;
  border-radius: 10rpx;
  margin: auto;
}
.itembox_1{
  height: 250rpx;
  width: 400rpx;
}
.itembox_1>text{
  color: #262626;
  height: 80rpx;
  line-height: 80rpx;
  margin-left: 15rpx;
}
.itembox_1_1{
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 24rpx;
  height: 40rpx;
  width: 360rpx;
  /* margin-left: 10rpx; */
  margin-top: 10rpx;
}
.itembox_1_1>view{
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  width: 110rpx;
  margin-left: 10rpx;
  background: #f5f5f5;
  color: #565656;
}
.itembox_1_2{
  height: 80rpx;
  width: 400rpx;
  margin-top: 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.itembox_1_2>text{
  width: 260rpx;
  color: #d6d6d6;
  font-size: 26rpx;
  margin-left: 10rpx;
}
.itembox_1_2>view{
  height:50rpx;
  line-height: 50rpx;
  font-size:28rpx;
  width: 170rpx;
  text-align: center;
  border-bottom-left-radius:30rpx;
  border-top-left-radius:30rpx;
  background: #d70050;
  color: #ffffff;
  margin-left: 20rpx;
}